<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hgcw.wiki.mapper.TestMapper">
    <select id="list" resultType="com.hgcw.wiki.domin.Test">
        select id, name, password
        from test
    </select>
    <select id="queryId" resultType="com.hgcw.wiki.domin.Test">
        select *
        from test
        where id = #{id}

    </select>
</mapper>
